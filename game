<body>

    <h1>Cricket Game</h1>

    <div class="game-container" id="gameContainer">
        <!-- Batsman -->
        <div class="batsman" id="batsman">
            <img src="data:image/webp;base64," alt="Batsman">
        </div>

        <!-- Bowler -->
        <div class="bowler" id="bowler">
            <img src="data:image/jpeg;base64," alt="Bowler">
        </div>

        <!-- Ball -->
        <div class="ball" id="ball"></div>
    </div>

    <div class="score-board">
        Score: <span id="score">0</span>
    </div>

    <button onclick="startBowl()">Bowl the Ball</button>

    <script>
        const ball = document.getElementById('ball');
        const gameContainer = document.getElementById('gameContainer');
        let score = 0;

function startBowl() {
    ball.style.display = 'block';
    let ballPos = 10;
    const ballInterval = setInterval(() => {
        if (ballPos >= 480) {
            clearInterval(ballInterval);
            hitBall(); // Trigger bat after ball reaches batsman
        } else {
            ball.style.top = `${ballPos}px`; // Corrected line
            ballPos += 10;
        }
    }, 50);
}

            ball.style.left = '50%';
            ball.style.transform = 'translateX(-50%)';
            ball.style.top = '50px';

        // Function to handle the bat interaction
        function hitBall() {
            const randomHit = Math.random(); // Random chance of hitting the ball
            if (randomHit > 0.5) {
                score += 4; // Hit a boundary
                alert("You hit a 4!");
            } else {
                score += 6; // Hit a six
                alert("You hit a 6!");
            }
            document.getElementById('score').innerText = score;
            resetBall(); // Reset ball after hit
        }

        // Function to reset ball
        function resetBall() {
            ball.style.display = 'none';
            ball.style.top = '50px'; // Reset ball to start position
        }
    </script>

</body>
</html>
