        }
            .game-container {
    width: 80%;
    height: 500px;
    margin: 0 auto;
    background-color: #32CD32;
    position: relative;
    border: 2px solid #000;
    overflow: hidden;
    border-radius: 50% ; /* Makes it oval */

        }

        .batsman, .bowler, .ball {
            position: absolute;
        }

        .batsman {
            width: 100px;
            height: 100px;
            bottom: 10px;
            left: 50%;
            transform: translateX(-50%);
        }

        .batsman img, .bowler img {
            width: 100%;
            height: 100%;
            object-fit: cover;
            border-radius: 50%;
        }

        .bowler {
            width: 100px;
            height: 100px;
            top: 10px;
            left: 50%;
            transform: translateX(-50%);
        }

